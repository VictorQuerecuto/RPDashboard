VA,wBAUA;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,aAAA,EAAA,CADA;AACA;AACA,QAAA,WAAA,EAAA,IAFA;AAGA,QAAA,gBAAA,EAAA,KAAA,IAAA,CAAA,WAHA;AAIA,QAAA,iBAAA,EAAA,KAAA,IAAA,CAAA;AAJA,OAAA;AAOA,MAAA,gBAAA,CAAA,MAAA,CAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAPA;AAQA,WAAA,KAAA,CAAA,cAAA;AACA,KA3BA;AA4BA,IAAA,KA5BA,mBA4BA;AACA,WAAA,KAAA,CAAA,cAAA;AACA;AA9BA,GAfA;AA+CA,EAAA,OA/CA,qBA+CA;AACA,SAAA,uBAAA;AACA;AAjDA,CAAA","sourcesContent":["<template>\n  <div class=\"overlay\">\n    <v-flex xs8>\n      <v-card>\n        <v-alert v-if=\"submitted\" type=\"success\">\n          Successfully Submitted Order\n        </v-alert>\n        <v-row>\n          <v-card-title>\n            <span>New Order</span>\n          </v-card-title>\n        </v-row>\n        <form>\n          <v-col cols=\"12\" sm=\"8\" md=\"6\">\n            <v-text-field\n              v-model=\"form.description\"\n              :rules=\"[rules.required]\"\n              label=\"Description\"\n              required\n            >\n            </v-text-field>\n            <v-select\n              :items=\"addresses\"\n              v-model=\"form.selectedAddress\"\n              item-text=\"address\"\n              label=\"Shipping Address\"\n              item-value=\"shippingAddressId\"\n              auto\n              required\n            ></v-select>\n          </v-col>\n        </form>\n        <div class=\"text-center my-2\">\n          <v-btn v-on:click=\"submitForm\" class=\"ma-2\" color=\"primary\" dark\n            >Submit\n            <v-icon dark right>mdi-checkbox-marked-circle</v-icon>\n          </v-btn>\n          <v-btn v-on:click=\"close\" class=\"ma-2\" dark>\n            Cancel\n            <v-icon dark right>mdi-close</v-icon>\n          </v-btn>\n        </div>\n      </v-card>\n    </v-flex>\n  </div>\n</template>\n\n<script>\nimport ShippingDataService from \"../services/ShippingDataService\";\nimport OrderDataService from \"../services/OrderDataService\";\n\nexport default {\n  name: \"shippingAddress-list\",\n  data() {\n    return {\n      addresses: [],\n      form: {\n        description: \"\",\n        selectedAddress: { shippingAddressId: 0, address: \"\" },\n      },\n      rules: {\n        required: (value) => !!value || \"Required.\",\n      },\n      submitted: false,\n    };\n  },\n  methods: {\n    retrieveShippingAddress() {\n      ShippingDataService.getAll()\n        .then((response) => {\n          this.addresses = response.data;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    submitForm() {\n      var data = {\n        orderStatusId: 1, //Order Status defaulted to 'Ordered'\n        shippedDate: null,\n        orderDescription: this.form.description,\n        shippingAddressId: this.form.selectedAddress,\n      };\n\n      OrderDataService.create(data)\n        .then((response) => {\n          console.log(response.data);\n          this.submitted = true;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n      this.$emit(\"toggle-modal\");\n    },\n    close() {\n      this.$emit(\"toggle-modal\");\n    },\n  },\n  mounted() {\n    this.retrieveShippingAddress();\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"postcss\" scoped>\n.overlay {\n  position: fixed;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  background-color: rgba(0, 0, 0, 0.3);\n  z-index: 100;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n</style>\n"],"sourceRoot":"src/components"}]}