A,yBACA;AACA,WAAA,YAAA,GAAA,CAAA,KAAA,YAAA;AACA,KAHA;AAIA,IAAA,cAJA,0BAIA,MAJA,EAIA;AAAA;;AACA,WAAA,aAAA,GAAA,MAAA;AAEA,MAAA,gBAAA,CAAA,mBAAA,CAAA,KAAA,aAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA,KAdA;AAeA,IAAA,WAfA,yBAeA;AACA,WAAA,cAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,CAAA,CAAA;AACA,KAnBA;AAoBA,IAAA,cApBA,0BAoBA,KApBA,EAoBA,KApBA,EAoBA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KAvBA,CAwBA;AACA;AACA;AACA;;AA3BA,GArBA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,SAAA,cAAA,CAAA,KAAA,aAAA;AACA;AApDA,CAAA","sourcesContent":["<template>\n  <div class=\"list row\">\n    <div class=\"col-md-4\">\n      <v-select\n        :items=\"orderStatus\"\n        v-model=\"currentFilter\"\n        item-text=\"name\"\n        label=\"Filter Order Status\"\n        item-value=\"orderStatusId\"\n        v-on:change=\"retrieveOrders(currentFilter)\"\n        dense\n        solo\n      ></v-select>\n    </div>\n    <div class=\"col-md-4\">\n      <h4>Orders Made</h4>\n      <modal v-if=\"isVisibility\" @toggle-modal=\"toggleModal\"></modal>\n      <button @click.prevent=\"toggleModal\">\n        Create Order\n      </button>\n      <ul class=\"list-group\">\n        <li\n          class=\"list-group-item\"\n          :class=\"{ active: index == currentIndex }\"\n          v-for=\"(order, index) in orders\"\n          :key=\"index\"\n          @click=\"setActiveOrder(order, index)\"\n        >\n          {{ order.orderDescription }}\n        </li>\n      </ul>\n    </div>\n    <div class=\"col-md-4\">\n      <div v-if=\"currentOrder\">\n        <h4>Order</h4>\n        <div>\n          <label>\n            <strong>Order Number:</strong>\n          </label>\n          {{ currentOrder.orderId }}\n        </div>\n        <div>\n          <label>\n            <strong>Description:</strong>\n          </label>\n          {{ currentOrder.orderDescription }}\n        </div>\n        <div>\n          <label>\n            <strong>Shipping Address:</strong>\n          </label>\n          {{ currentOrder.shippingAddressId }}\n        </div>\n        <div>\n          <label>\n            <strong>Status:</strong>\n          </label>\n          {{ currentOrder.orderStatusId }}\n        </div>\n        <v-btn class=\"ma-2\" dark>\n          Locate\n          <v-icon right>mdi-close</v-icon>\n        </v-btn>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OrderDataService from \"../services/OrderDataService\";\nimport modal from \"../components/modal\";\n\nexport default {\n  components: {\n    modal,\n  },\n  name: \"orders-list\",\n  data() {\n    return {\n      isVisibility: false,\n      orders: [],\n      currentOrder: null,\n      currentIndex: -1,\n      description: \"\",\n      currentFilter: 1,\n      orderStatus: [\n        { orderStatusId: 1, name: \"Ordered\" },\n        { orderStatusId: 2, name: \"Processing\" },\n        { orderStatusId: 3, name: \"Shipped\" },\n        { orderStatusId: 4, name: \"Recieved\" },\n      ],\n    };\n  },\n  methods: {\n    toggleModal() {\n      this.isVisibility = !this.isVisibility;\n    },\n    retrieveOrders(filter) {\n      this.currentFilter = filter;\n\n      OrderDataService.findByOrderStatusId(this.currentFilter)\n        .then((response) => {\n          this.orders = response.data;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    refreshList() {\n      this.retrieveOrders();\n      this.currentOrder = null;\n      this.currentIndex = -1;\n    },\n    setActiveOrder(order, index) {\n      this.currentOrder = order;\n      this.currentIndex = index;\n    },\n    //setFilter(filter) {\n    //  this.currentFilter = filter;\n    //  console.log(filter);\n    //},\n  },\n  mounted() {\n    this.retrieveOrders(this.currentFilter);\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"sourceRoot":"src/components"}]}